<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">t2.gstatic.com</domain>
        <domain includeSubdomains="true">t1.gstatic.com</domain>
        <domain includeSubdomains="true">t3.gstatic.com</domain>
        <domain includeSubdomains="true">t0.gstatic.com</domain>
    </domain-config>
    <base-config>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </base-config>
</network-security-config>